{
  "name": "backend-api",
  "version": "0.0.3",
  "description": "monorepo API",
  "author": "gobeam <patrickndayambaje4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:beapi": "nest build backend-api",
    "build:feapi": "nest build frontend-api",
    "build:queue": "nest build queue",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "start:beapi": "cross-env NODE_ENV=development nest start backend-api",
    "start:beapi:prod": "cross-env NODE_ENV=development node dist/apps/backend-api/src/main",
    "start:beapi:dev": "cross-env NODE_ENV=development nest start backend-api -w",
    "start:feapi": "cross-env NODE_ENV=development  nest start frontend-api",
    "start:feapi:prod": "cross-env NODE_ENV=development node dist/apps/frontend-api/src/main",
    "start:feapi:dev": "cross-env NODE_ENV=development  nest start frontend-api -w",
    "start:queue:dev": "cross-env NODE_ENV=development nest start queue-bull -w",
    "start:queue:prod": "cross-env NODE_ENV=development node dist/apps/queue-bull/src/main",
    "start:queue": "cross-env NODE_ENV=development nest start queue-bull",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/backend-api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d apps/backend-api/src/config/ormconfig.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./apps/backend-api/src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "seed-typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d apps/backend-api/src/config/seeder-config.ts",
    "seed:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./apps/backend-api/src/database/seeds/$npm_config_name",
    "seed:run": "npm run seed-typeorm -- migration:run",
    "seed:revert": "npm run seed-typeorm -- migration:revert",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run lint",
    "prepare": "husky install",
    "audit:fix": "yarn-audit-fix"
  },
